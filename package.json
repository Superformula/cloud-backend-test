{
  "name": "devclericuzzi-cloud-backend-test",
  "version": "0.0.0",
  "description": "SuperFormula recruitment process test",
  "main": "index.js",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "post-merge": "npm i",
      "post-checkout": "npm i"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "pkg/dist",
      "terraform",
      "node_modules"
    ]
  },
  "lint-staged": {
    "!(*test).ts": [
      "eslint --ext .ts .",
      "jest --bail --findRelatedTests"
    ]
  },
  "scripts": {
    "test": "jest --runInBand",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts . --fix",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devclericuzzi/cloud-backend-test.git"
  },
  "keywords": [],
  "author": "Pedro Clericuzzi <dev.clericuzzi@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/devclericuzzi/cloud-backend-test/issues"
  },
  "homepage": "https://github.com/devclericuzzi/cloud-backend-test#readme",
  "devDependencies": {
    "@aws-sdk/types": "^3.15.0",
    "@commitlint/cli": "^12.1.3",
    "@commitlint/config-conventional": "^12.1.3",
    "@joi/date": "^2.1.0",
    "@types/aws-lambda": "^8.10.76",
    "@types/jest": "^26.0.23",
    "@types/joi": "^17.2.3",
    "@types/node": "^15.3.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jest": "^24.3.6",
    "husky": "^3.1.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
