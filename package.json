{
  "name": "cloud-backend-test",
  "version": "1.0.0",
  "description": "lucas fonseca project",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucaslimafons/cloud-backend-test.git"
  },
  "author": "Lucas Lima Fonseca",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lucaslimafons/cloud-backend-test/issues"
  },
  "homepage": "https://github.com/lucaslimafons/cloud-backend-test#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "cross-env NODE_ENV=production eslint",
      "pretty-quick --staged --write"
    ]
  },
  "scripts": {
    "test": "npm cache clean --force && sequelize db:drop --env test || true && sequelize db:create --env test && npm run migrate test && npm run seeder test && jest src/.*/*.test.ts --bail --detectOpenHandles --maxWorkers=1 --testTimeout 10000",
    "coverage": "jest --coverage",
    "start": "npm run migrate development && npm run seeder development && cross-env NODE_ENV=development tsnd -r tsconfig-paths/register -r ts-node --transpile-only --tree-kill --respawn --ignore-watch node_modules ./src/server/server.ts",
    "lint": "cross-env NODE_ENV=production eslint . --quiet",
    "lint:staged": "lint-staged",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "prod:start": "yarn run migrate production && yarn run seeder production && cross-env NODE_ENV=production node ./dist/server/cluster.js",
    "migrate": "cross-env sequelize db:migrate --env",
    "seeder": "cross-env sequelize db:seed:all --env",
    "undo_seeder": "cross-env sequelize db:seed:undo --env",
    "undo_migrate": "cross-env sequelize db:migrate:undo --env"
  },
  "dependencies": {
    "@types/express": "^4.17.11",
    "@types/graphql-upload": "^8.0.4",
    "@types/http-server": "^0.10.1",
    "@types/jest": "^26.0.23",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.0.2",
    "@types/supertest": "^2.0.11",
    "apollo-server-express": "^2.24.0",
    "aws-sdk": "^2.904.0",
    "compression": "^1.7.4",
    "dotenv": "^9.0.0",
    "eslint": "^7.26.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "faker": "^5.5.3",
    "graphql": "^15.5.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-query-complexity": "^0.8.1",
    "graphql-tools": "^7.0.4",
    "graphql-upload": "^12.0.0",
    "husky": "^6.0.0",
    "i18n": "^0.13.3",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.2",
    "sequelize-typescript": "^2.1.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-unused-imports": "^1.1.1"
  },
  "eslintIgnore": [
    "!.eslintrc.js",
    "node_modules",
    "/docs/scripts"
  ],
  "_moduleAliases": {
    "@": "src/"
  }
}
