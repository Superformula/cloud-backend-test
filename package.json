{
  "name": "devclericuzzi-cloud-backend-test",
  "version": "0.0.0",
  "description": "SuperFormula recruitment process test",
  "main": "index.js",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "post-merge": "npm i",
      "post-checkout": "npm i"
    }
  },
  "lint-staged": {
    "!(*test).js": [
      "npm run lint",
      "jest --bail --findRelatedTests"
    ]
  },
  "scripts": {
    "test": "jest",
    "testCoverage": "jest --collectCoverage",
    "lint": "eslint --ext .js,.test.js .",
    "lint:fix": "eslint --ext .js,.test.js . --fix",
    "commit": "git-cz",
    "build-ts": "rimraf dist && npx tsc && npm run createLayer",
    "createLayer": "rimraf .\\app\\layers && npx mkdirp .\\app\\layers\\nodejs && copyfiles .\\.npmrc .\\app\\layers\\nodejs && cd .\\app\\layers\\nodejs && npm init -y && npm i joi @joi/date axios uuid && npm run importProjectPackage",
    "build-lambdas": "rimraf .\\app\\lambdas\\crud.js && rimraf .\\app\\lambdas\\geolocation.js && npx tsc .\\app\\lambdas\\crud.ts .\\app\\lambdas\\geolocation.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devclericuzzi/cloud-backend-test.git"
  },
  "keywords": [],
  "author": "Pedro Clericuzzi <dev.clericuzzi@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/devclericuzzi/cloud-backend-test/issues"
  },
  "homepage": "https://github.com/devclericuzzi/cloud-backend-test#readme",
  "devDependencies": {
    "@aws-sdk/types": "^3.15.0",
    "@babel/core": "^7.14.2",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^12.1.3",
    "@commitlint/config-conventional": "^12.1.3",
    "@devclericuzzi/cloud-backend-lib": "^1.0.3",
    "@joi/date": "^2.1.0",
    "@types/aws-lambda": "^8.10.76",
    "@types/jest": "^26.0.23",
    "@types/joi": "^17.2.3",
    "@types/node": "^15.3.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "aws-sdk": "^2.907.0",
    "babel-jest": "^26.6.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.26.0",
    "eslint-plugin-jest": "^24.3.6",
    "husky": "^3.1.0",
    "jest": "^26.6.3",
    "joi": "^17.4.0",
    "lint-staged": "^11.0.0",
    "mkdirp": "^1.0.4",
    "nodemon": "^2.0.7",
    "rewire": "^5.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^9.0.2"
  }
}
