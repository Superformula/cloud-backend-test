		"test": "jest"
	},
	"devDependencies": {
		"@types/jest": "^26.0.23",
		"@typescript-eslint/eslint-plugin": "^4.22.0",
		"@typescript-eslint/parser": "^4.22.0",
		"aws-sdk": "^2.897.0",
		"cpy-cli": "^3.1.1",
