		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.0",
		"jest": "^26.6.3",
		"jest-aws-sdk-mock": "^1.0.2",
		"prettier": "^2.2.1",
		"trace-pkg": "^0.3.4",
		"ts-jest": "^26.5.5",
		"typescript": "^4.2.4"
	},
	"dependencies": {
		"@graphql-tools/graphql-file-loader": "^6.2.7",
		"@graphql-tools/load": "^6.2.8",
		"apollo-server-lambda": "^2.23.0",
		"graphql": "^15.5.0"
	}
